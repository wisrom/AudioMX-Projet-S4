<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="main_design_wrapper"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
main_design_wrapper
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="main_design_wrapper">
        <sdx:configuration sdx:name="main_design_wrapper"
                           sdx:displayName="main_design_wrapper"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>main_design_wrapper</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="main_design_wrapper/boot/main_design_wrapper.bif"
                            sdx:readme="main_design_wrapper/boot/generic.readme"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on microblaze_0"
                                sdx:cpuType="microblaze_0"
                                sdx:cpuInstance="microblaze_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="main_design_wrapper/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="main_design_wrapper/standalone_domain/bspinclude/include"
                        sdx:bspConfig="main_design_wrapper/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
